    "socket.io": "^4.6.0",
    "swagger-ui-express": "^4.6.0",
    "tough-cookie": "^4.1.2",
    "typeorm": "^0.3.12",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^9.3.7",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "~4.17.3",
    "@types/jest": "27.5.0",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
